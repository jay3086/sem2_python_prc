   "source": [
    "\n",
    "n = int(input(\"enter a number : \"))\n",
    "flag = False\n",
    "for i in range(2,n):\n",
    "    if(n%i==0):\n",
    "        flag = True\n",
    "    else:\n",
    "        flag = False\n",
    "if flag==True:\n",
    "    print(\"number is non prime\")\n",
    "else:\n",
    "    print(\"number is prime\")"
